config AUDIENCE_A1028
	tristate "A1028 Voice Processor Driver"
	default y
	help
	  Say Y here to enable a1028 voice processor support.

config AUDIENCE_ES305B
	tristate "ES305B Voice Processor Driver"
	default y
	help
	  Say Y here to enable es305b Voice Processor  support.
	  
config  REGULATOR_AUDIO_CRYSTAL
	bool "Regulator for audio crystal "
	depends on MACH_M030 && AUDIENCE_A1028
	help
	  Say Y for use the regulator of audio crystal
